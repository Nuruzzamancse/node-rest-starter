{
  "name": "saftety-count-web-portal",
  "description": "A boilerplate for creating RESTful APIs in Node.js using Express and Sequlize.",
  "version": "0.6.0",
  "author": "Sk Arif",
  "contributor": "Nuruzzaman Khan Nabwab",
  "license": "MIT",
  "repository": "github:skarif2/node-rest-starter",
  "homepage": "https://github.com/skarif2/node-rest-starter#readme",
  "keywords": [
    "node"
  ],
  "engines": {
    "node": ">=10.3.0",
    "npm": ">=6.1.0"
  },
  "dependencies": {
    "@skarif2/logger": "^0.3.0",
    "basic-auth": "^2.0.1",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.5.3",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "consola": "^2.3.1",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-jwt": "^5.3.1",
    "express-validation": "^1.0.2",
    "helmet": "^3.15.0",
    "http-status": "^1.3.1",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.4.0",
    "lodash": "^4.17.11",
    "method-override": "^3.0.0",
    "mysql2": "^1.5.3",
    "sequelize": "^5.9.4",
    "sequelize-paginate": "^1.1.6"
  },
  "devDependencies": {
    "apidoc": "^0.17.7",
    "coveralls": "^3.0.2",
    "eslint": "^5.11.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.1.2",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^0.14.3",
    "jest": "^23.6.0",
    "nodemon": "^1.18.9",
    "supertest": "^3.3.0",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-3": "^6.24.1",
    "faker": "^4.1.0",
    "lodash.random": "^3.2.0",
    "lodash.times": "^4.3.2",
    "sequelize-cli": "^4.0.0"
  },
  "scripts": {
    "start": "npx nodemon --exec babel-node ./src/index.js",
    "start:watch": "nodemon ./src/index.js",
    "start:debug": "node ./src/index.js",
    "lint": "eslint --ext=.js src || true",
    "test": "SET NODE_ENV=test MONGO_HOST=localhost MONGO_PORT=27017 MONGO_USERNAME=uncount MONGO_PASSWORD=uncount",
    "test:watch": "SET  NODE_ENV=test MONGO_HOST=mongodb://localhost/nrs-test jest --watch",
    "test:coverage": "NODE_ENV=test MONGO_HOST=mongodb://localhost/nrs-test jest --coverage",
    "precommit": "eslint --ext=.js src && npm run test",
    "apidoc": "apidoc -i ./src -f .route.js -o ./docs"
  },
  "apidoc": {
    "title": "Safety Count Web Portal",
    "url": "{baseURL}"
  }
}
